@page
@model WebHost.Areas.Administration.Pages.AccessDeniedModel
@{
    ViewData["Title"] = "عدم دسترسی";
}


        <div >
            <div class="container-fluid">                
                <div class="row">
                    
                    <div class="col-md-12">
                        <div class="logo-con m-t-10 m-b-10">
                        <img src="~/Admin-Theme/assets/images/logo.png" class="center-block img-responsive">
                        </div><!-- /.logo-con -->

                        <hr>

                        <p class="text-center m-t-40">
                            <i class="icon-shield border img-circle font-xxxlg p-20"></i>
                        </p>
                        <h1 class="page-error m-t-30">
                            <sup>خطای</sup>
                            404
                        </h1>
                        <p class="text-center font-lg m-b-20">صفحه مورد نظر شما یافت نشد.</p>
                        <form role="form">
                            <div class="form-body">
                                <div class="input-group round">
                                    <input type="text" class="form-control" placeholder="جستجو به شما کمک می کند..."> 
                                    <span class="input-group-btn">
                                        <button class="btn btn-info" type="button">
                                            <i class="icon-magnifier"></i> 
                                        </button>
                                    </span>
                                </div><!-- ./input-group -->
                            </div><!-- /.form-body -->
                        </form>

                        <hr class="m-b-30 m-t-30">
                        <a asp-page="/Index" class="btn btn-default btn-round btn-block">
                            بازگشت    
                            <i class="icon-arrow-left font-lg"></i>
                        </a>
                    </div><!-- /.col-md-12 -->                    
                </div><!-- /.row -->
            </div><!-- /.container-fluid -->
        </div><!-- /.modal-page -->
        <!-- END WRAPPER -->
        
 @section Scripts
 {
     <script src="~/Admin-Theme/assets/plugins/jquery/dist/jquery-3.6.1.min.js"></script>
     <script src="~/Admin-Theme/assets/plugins/jquery-migrate/jquery-migrate-1.2.1.min.js"></script>
     <script src="~/Admin-Theme/assets/plugins/bootstrap/bootstrap5/js/bootstrap.bundle.min.js"></script>
     <script src="~/Admin-Theme/assets/plugins/paper-ripple/dist/PaperRipple.min.js"></script>
     <script src="~/Admin-Theme/assets/plugins/sweetalert2/dist/sweetalert2.min.js"></script>
     <script src="~/Admin-Theme/assets/plugins/iCheck/icheck.min.js"></script>
     <script src="~/Admin-Theme/assets/plugins/jquery-validation/dist/jquery.validate.min.js"></script>
     <script src="~/Admin-Theme/assets/plugins/jquery-validation/src/localization/messages_fa.js"></script>
     <script src="~/Admin-Theme/assets/js/core.js"></script>
 }

        
                

